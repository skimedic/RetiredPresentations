<Window x:Class="WPFDemo.E_DataBinding.Views._Launcher"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:DataBinding="clr-namespace:WPFDemo.E_DataBinding.Views" 

        Title="_Launcher" Height="300" Width="300">
    <Grid>
		<StackPanel Grid.Column="0" Name="stackPanel1" Width="Auto" ButtonBase.Click="MenuItem_Click">
			<Button Height="Auto" Name="cmdElementBinding" Width="Auto" Content="Element Binding" Tag="{x:Type DataBinding:A_ElementBinding}" />
			<Button Height="Auto" Name="cmdGridView" Width="Auto" Content="Grid View" Tag="{x:Type DataBinding:BasicGridView}" />
			<Button Height="Auto" Name="cmdCheckedLists" Width="Auto" Content="Checked Lists" Tag="{x:Type DataBinding:CheckedLists}" />
			<Button Height="Auto" Name="cmdTreeView" Width="Auto" Content="Tree View" Tag="{x:Type DataBinding:TreeView}" />
			<Button Height="Auto" Name="cmdCollectionBinding" Width="Auto" Content="Collections" Tag="{x:Type DataBinding:E_CollectionBinding}" />
			<Button Height="Auto" Name="cmdTemplates" Width="Auto" Content="DataTemplates" Tag="{x:Type DataBinding:F_DataTemplates}" />
			<Button Height="Auto" Name="cmdMasterDetail" Width="Auto" Content="Master Detail" Tag="{x:Type DataBinding:G_MasterDetail}" />
		</StackPanel>

	</Grid>
</Window>
